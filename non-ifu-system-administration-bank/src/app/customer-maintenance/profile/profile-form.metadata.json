        "fieldGroup": [
            {
                "key": "orgVO.orgName",
                "type": "input",
                "className": "flex-gt-sm-70 flex-sm-50 flex-xs",
                "templateOptions": {
                    "type": "text",
                    "label": "CUSTOMER_ABBV_NAME",
                    "required": true,
                    "maxlength": 20,
                    "onKeydown": "this.options.formControl.$invalid = false;",
                    "pattern": "([a-zA-Z]+)"
                },
                "expressionProperties": {
                    "templateOptions.label": "'CUSTOMER_ABBV_NAME'|translate",
                    "templateOptions.readonly": "formState.displayState === 'update'"
                },
                "asyncValidators": {
                    "validatorUnique": "formState.validator.GenericFormValidationService.validateUniqueAsync('/organization/ba/customer/unique', {'abbvName': $viewValue})"
                },
                "validation": {
                    "messages": {
                        "pattern": "'FORM.REGEX_ALPHA_MESSAGE'|translate",
                        "validatorUnique": "'FORM.ERROR_MESSAGE_UNIQUE' | translate: { label: this.to.label }"
                    }
                },
                "modelOptions": {
                    "updateOn": "change"
                }
            },
            {
                "key": "orgVO.name",
                "type": "input",
                "className": "flex-gt-sm-30 flex-sm-50 flex-xs",
